{
  "extends": "eslint:recommended",

  "parser": "babel-eslint",

  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },

  "rules": {
    "semi": 1,
    "strict": 0,
    "quotes": [2, "single"],
    "no-underscore-dangle": [0],
    "new-cap": 0,
    "no-shadow": 0,
    "no-console": 0,
    "comma-dangle": 0
  }
}
